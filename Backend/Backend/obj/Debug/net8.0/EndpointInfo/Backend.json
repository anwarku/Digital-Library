  "components": {
    "schemas": {
      "AddBookDto": {
        "required": [
          "author",
          "publishYear",
          "stock",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "minLength": 10,
            "type": "string"
          },
          "author": {
            "minLength": 1,
            "type": "string"
          },
          "publisher": {
            "type": "string",
            "nullable": true
          },
          "publishYear": {
            "maximum": 2025,
            "minimum": 1990,
            "type": "integer",
            "format": "int32"
          },
          "isbn": {
            "minLength": 10,
            "type": "string",
            "nullable": true
          },
          "stock": {
            "maximum": 50,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "AddMemberDto": {
        "required": [
          "gender",
          "name",
          "phone"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "gender": {
            "minLength": 1,
            "type": "string"
          },
          "phone": {
            "minLength": 1,
            "type": "string"
          },
          "job": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Book": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "author": {
            "type": "string",
            "nullable": true
          },
          "publisher": {
            "type": "string",
            "nullable": true
          },
          "publishYear": {
            "type": "integer",
            "format": "int32"
          },
          "isbn": {
            "type": "string",
            "nullable": true
          },
          "stock": {
            "type": "integer",
            "format": "int32"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "detailTransactions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DetailTransaction"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DetailTransaction": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "transactionId": {
            "type": "string",
            "nullable": true
          },
          "bookCode": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Member": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "phone": {
            "type": "string",
            "nullable": true
          },
          "gender": {
            "type": "string",
            "nullable": true
          },
          "job": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Transaction": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "borrowDate": {
            "type": "string",
            "format": "date"
          },
          "returnDate": {
            "type": "string",
            "format": "date",
            "nullable": true
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "memberId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "UpdateBookDto": {
        "required": [
          "code",
          "stock"
        ],
        "type": "object",
        "properties": {
          "code": {
            "minLength": 1,
            "type": "string"
          },
          "stock": {
            "maximum": 50,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
